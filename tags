Character	lib/character.rb	/^class Character$/;"	c
Eyes	lib/character.rb	/^  class Eyes$/;"	c	class:Character
InfravisionPotion	lib/infravision_potion.rb	/^class InfravisionPotion$/;"	c
affect	lib/infravision_potion.rb	/^  def affect(character)$/;"	f	class:InfravisionPotion
drink	lib/character.rb	/^  def drink(potion)$/;"	f	class:Character
enchant_eyes_with	lib/character.rb	/^  def enchant_eyes_with(enchantment)$/;"	f	class:Character
initialize	lib/character.rb	/^  def initialize$/;"	f	class:Character
look	lib/character.rb	/^    def look$/;"	f	class:Character.Eyes
look	lib/character.rb	/^  def look$/;"	f	class:Character
observe	lib/character.rb	/^  def observe$/;"	f	class:Character
